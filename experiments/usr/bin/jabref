#!/bin/sh

# Include the wrappers utility script
. /usr/lib/java-wrappers/java-wrappers.sh

# We need a java6 runtime (openjdk[876], oracle[87], sun6).
find_java_runtime java6

find_jars \
	jabref \
	JPFCodeGenerator-rt \
	antlr \
	antlr3 \
	commons-logging \
	glazedlists \
	jempbox \
	jgoodies-common \
	jgoodies-forms \
	jgoodies-looks \
	jpf \
	jpf-boot \
	microba \
	mysql-connector-java \
	pdfbox \
	postgresql \
	spin

run_java net.sf.jabref.JabRefMain "$@"
